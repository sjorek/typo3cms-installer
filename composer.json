{
	"autoload" : {
		"psr-0" : {
			"Sj" : "src"
		}
	},
	"name" : "sj/typo3cms-installers",
	"type" : "composer-plugin",
	"license" : "GPL-3.0+",
	"require" : {
		"php" : ">=5.4",
		"composer-plugin-api" : "1.0.0",
		"lw/typo3cms-installers" : "dev-master"
	},
	"extra" : {
		"class" : "Sj\\Composer\\Plugins\\TYPO3CMSInstallerPlugin"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Sj\\Composer\\Scripts\\TYPO3CMSInstallerScript::dispatchCommand"
		],
		"post-update-cmd" : [
			"Sj\\Composer\\Scripts\\TYPO3CMSInstallerScript::dispatchCommand"
		],
		"post-package-install" : [
			"Sj\\Composer\\Scripts\\TYPO3CMSInstallerScript::dispatchPackage"
		],
		"post-package-update" : [
			"Sj\\Composer\\Scripts\\TYPO3CMSInstallerScript::dispatchPackage"
		],
		"post-package-uninstall" : [
			"Sj\\Composer\\Scripts\\TYPO3CMSInstallerScript::dispatchPackage"
		]
	},
	"repositories" : [{
			"type" : "git",
			"url" : "git@github.com:lightwerk/typo3cms-installers.git"
		}
	],
	"minimum-stability" : "dev"
}