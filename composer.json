{
	"autoload" : {
		"psr-0" : {
			"Sj" : "src"
		}
	},
	"name" : "sj/typo3cms-installers",
	"type" : "composer-plugin",
	"license" : "GPL-3.0+",
	"require" : {
		"php" : ">=5.4",
		"composer-plugin-api" : "1.0.0",
		"lw/typo3cms-installers" : "dev-master"
	},
	"require-dev": {
		"sj/satis-github-scripts": "dev-master",
	},
	"extra" : {
		"class" : "Sj\\Composer\\Plugins\\TYPO3CMSInstallerPlugin"
	},
	"scripts" : {
		"post-install-cmd" : [
			"[-x vendor/bin/satis-github-publisher.sh ] && vendor/bin/satis-github-publisher.sh"
		],
		"post-update-cmd" : [
			"[-x vendor/bin/satis-github-publisher.sh ] && vendor/bin/satis-github-publisher.sh"
		]
	},
	"repositories" : [{
			"type" : "git",
			"url" : "git@github.com:lightwerk/typo3cms-installers.git"
		}, {
			"type": "composer",
			"url": "http://sjorek.github.io/satis-github-scripts"
		}
	],
	"minimum-stability" : "dev"
}